	<queryString>
		<![CDATA[SELECT
    SUM(a.amount) AS "TOTAL_AMOUNT",
    SUM(
        CASE
            WHEN c.employee_bank IS NULL THEN a.amount
            ELSE 0
        END
    ) AS "CASH_TOTAL_NULL",
    SUM(
        CASE
            WHEN c.employee_bank IS NOT NULL THEN a.amount
            ELSE 0
        END
    ) AS "CASH_TOTAL_ONE",
    COUNT(DISTINCT c.emp_code) AS "TOTAL_EMPLOYEES",
    COUNT(DISTINCT CASE WHEN c.employee_bank IS NOT NULL THEN c.emp_code END) AS "EMP_BANK_COUNT",
    COUNT(DISTINCT CASE WHEN c.employee_bank IS NULL THEN c.emp_code END) AS "EMP_CASH_COUNT"
FROM
    payroll_employee c
    LEFT JOIN payroll_payprocess a 
        ON a.employee_code = c.emp_code
        AND a.earn_code = 'ER999'
        AND a.pay_cycle = COALESCE($P{P1}, a.pay_cycle)
        AND a.pay_month = COALESCE($P{P2}, a.pay_month)
WHERE
    a.comp_code = $P{P0}
    AND a.comp_code = c.comp_code
    AND c.process_cycle = a.pay_cycle]]>
	</queryString>
