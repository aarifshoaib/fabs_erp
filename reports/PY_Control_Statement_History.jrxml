	<subDataset name="Dataset1" uuid="1edb0eca-ab1b-4987-b9bb-8a3a49a1d630">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="Apex"/>
		<parameter name="P0" class="java.lang.String"/>
		<parameter name="P1" class="java.lang.String"/>
		<parameter name="P2" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
    SUM(a.amount) AS total_amount,
    SUM(CASE WHEN c.emp_bank IS NULL AND a.earn_type != 'Net' THEN a.amount ELSE 0 END) AS cash_total,
    SUM(CASE WHEN c.emp_bank IS NOT NULL AND a.earn_type != 'Net' THEN a.amount ELSE 0 END) AS bank_total
FROM
    tbl_erp_pay_process a, 
    tbl_erp_gmas_common_master b, 
    tbl_erp_pmas_employee_master c
WHERE
    a.comp_code = $P{P0}
    AND a.comp_code = b.comp_code
    AND a.comp_code = c.comp_code
    AND a.pay_cycle = NVL($P{P1}, a.pay_cycle)
    AND a.pay_month = NVL($P{P2}, a.pay_month)
    AND a.pay_cycle = b.common_master_id
    AND b.base_type = 'PROCESS CYCLE'
    AND a.pay_cycle = c.process_cycle]]>
		</queryString>
		<field name="TOTAL_AMOUNT" class="java.math.BigDecimal"/>
		<field name="CASH_TOTAL" class="java.math.BigDecimal"/>
		<field name="BANK_TOTAL" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="P0" class="java.lang.String"/>
	<parameter name="P1" class="java.lang.String"/>
	<parameter name="P2" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
    CASE 
        WHEN UPPER(a.earn_type) LIKE 'EARN%' THEN 'EARNINGS'
        WHEN UPPER(a.earn_type) LIKE 'DEDU%' THEN 'DEDUCTIONS'
        WHEN UPPER(a.earn_type) LIKE 'NET' THEN 'NET'
        ELSE ''
    END AS "EARN_TYPE",
    a.pay_cycle AS "PAY_CYCLE",
    a.earn_reports AS "EARN_REPORTS",
    TO_CHAR(TO_DATE(a.pay_month, 'MMYYYY'), 'MON-YYYY') AS "PAY_MONTH",
    a.earn_code AS "EARN_CODE",
    SUM(a.amount) AS "TOTAL_AMOUNT"
FROM
    payroll_payprocessarchieve a
WHERE
    a.comp_code = $P{P0}
    AND a.pay_cycle = COALESCE($P{P1}, a.pay_cycle)
    AND a.pay_month = COALESCE($P{P2}, a.pay_month)
    AND a.earn_code NOT IN ('ER900', 'DD900')
GROUP BY
    a.earn_type,
    a.earn_code,
    a.pay_cycle,
    a.pay_month,
    a.earn_reports
ORDER BY
    CASE 
        WHEN UPPER(a.earn_type) LIKE 'EARN%' THEN 1
        WHEN UPPER(a.earn_type) LIKE 'DEDU%' THEN 2
        WHEN UPPER(a.earn_type) LIKE 'NET' THEN 3
        ELSE 4
    END]]>
	</queryString>
