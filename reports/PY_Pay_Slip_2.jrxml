		<parameter name="P0" class="java.lang.String"/>
		<parameter name="P1" class="java.lang.String"/>
		<parameter name="P2" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
    b.earn_code AS "EARN_CODE",
    b.earn_reports AS "EARN_REPORTS",
    b.earn_type AS "EARN_TYPE",
    b.amount AS "AMOUNT"
FROM
    payroll_employee a
    LEFT JOIN payroll_payprocess b 
        ON a.emp_code = b.employee_code
        AND a.comp_code = b.comp_code
WHERE
    a.comp_code = $P{P0}
    AND a.emp_code = COALESCE($P{P1}, a.emp_code)
    AND b.pay_month = COALESCE($P{P2}, b.pay_month)
    AND UPPER(b.earn_type) = 'EARNINGS'
    AND b.earn_code != 'ER900']]>
		</queryString>
		<field name="EARN_CODE" class="java.lang.String"/>
		<field name="EARN_REPORTS" class="java.lang.String"/>
		<field name="EARN_TYPE" class="java.lang.String"/>
		<field name="AMOUNT" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="Dataset_2" uuid="3d0bc0ea-1722-45f9-bf93-9c127e7a3cb8">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="POSTGRES"/>
		<parameter name="P0" class="java.lang.String"/>
		<parameter name="P1" class="java.lang.String"/>
		<parameter name="P2" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
    b.earn_code AS "EARN_CODE",
    b.earn_reports AS "EARN_REPORTS",
    b.earn_type AS "EARN_TYPE",
    b.amount AS "AMOUNT"
FROM
    payroll_employee a
    LEFT JOIN payroll_payprocess b 
        ON a.emp_code = b.employee_code
        AND a.comp_code = b.comp_code
WHERE
    a.comp_code = $P{P0}
    AND a.emp_code = COALESCE($P{P1}, a.emp_code)
    AND b.pay_month = COALESCE($P{P2}, b.pay_month)
    AND UPPER(b.earn_type) IN ('DEDUCTIONS', 'DEDUCTIONS1')
    AND b.earn_code != 'DD900']]>
		</queryString>
		<field name="EARN_CODE" class="java.lang.String"/>
		<field name="EARN_REPORTS" class="java.lang.String"/>
		<field name="EARN_TYPE" class="java.lang.String"/>
		<field name="AMOUNT" class="java.math.BigDecimal"/>
	</subDataset>
	<subDataset name="Dataset_3" uuid="ba3a7ffc-cba0-4fb1-9f44-066c27e64e6c">
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="POSTGRES"/>
		<parameter name="P0" class="java.lang.String"/>
		<parameter name="P1" class="java.lang.String"/>
		<parameter name="P2" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
    b.earn_code AS "EARN_CODE",
    b.earn_reports AS "EARN_REPORTS",
    b.earn_type AS "EARN_TYPE",
    b.amount AS "AMOUNT"
FROM
    payroll_employee a
    LEFT JOIN payroll_payprocess b 
        ON a.emp_code = b.employee_code
        AND a.comp_code = b.comp_code
WHERE 
    a.comp_code = '1000'
    AND a.emp_code = COALESCE($P{P1}, a.emp_code)
    AND b.pay_month = COALESCE($P{P2}, b.pay_month)
    AND b.earn_code IN ('ER900', 'DD900', 'ER999')
ORDER BY
    CASE 
        WHEN b.earn_code = 'ER900' THEN 1
        WHEN b.earn_code = 'DD900' THEN 2
        WHEN b.earn_code = 'ER999' THEN 3
        ELSE 4
    END]]>
		</queryString>
		<field name="EARN_CODE" class="java.lang.String"/>
		<field name="EARN_REPORTS" class="java.lang.String"/>
		<field name="EARN_TYPE" class="java.lang.String"/>
		<field name="AMOUNT" class="java.math.BigDecimal"/>
	</subDataset>
	<parameter name="P0" class="java.lang.String"/>
	<parameter name="P2" class="java.lang.String"/>
	<parameter name="P1" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
    a.emp_code AS "EMP_CODE",
    a.emp_name AS "EMP_NAME",
    a.designation AS "DESIG_CODE",
    a.department AS "DEPARTMENT",
    a.basic_pay AS "BASIC_PAY",
    b.process_description AS "PROCESS_DESCRIPTION",
    b.pay_process_month AS "PAY_PROCESS_MONTH",
    a.employee_bank,
    a.bank_branch,
    a.account_no,
    a.iban_number,
    a.date_of_join,
    TO_CHAR(TO_DATE(b.pay_process_month, 'MMYYYY'), 'MON/YYYY') AS "FORMATTED_PAY_PROCESS_MONTH",
    
    -- Sum of additional earnings as allowances excluding specific earn_reports
    SUM(CASE 
        WHEN UPPER(c.earn_type) = 'EARNINGS' 
             AND c.earn_reports NOT IN ('EARN OT1', 'EARN OT2', 'EARN BASIC', 'NET PAY') 
        THEN c.amount 
        ELSE 0 
    END) AS "ALLOWANCES",

	 SUM(CASE 
        WHEN UPPER(c.earn_type) = 'EARNINGS' 
             AND c.earn_reports IN ('EARN OT1', 'EARN OT2') 
        THEN c.amount 
        ELSE 0 
    END) AS "OT",
	
    -- Sum of deductions
    SUM(CASE 
        WHEN UPPER(c.earn_type) IN ('DEDUCTIONS', 'DEDUCTIONS1') 
        THEN c.amount 
        ELSE 0 
    END) AS "DEDUCTIONS"

FROM
    payroll_employee a
    INNER JOIN payroll_paycyclemaster b 
        ON a.process_cycle = b.process_cycle
        AND a.comp_code = b.comp_code
    RIGHT JOIN payroll_payprocess c 
        ON a.emp_code = c.employee_code
        AND c.pay_month = b.pay_process_month
        AND a.comp_code = c.comp_code
WHERE
    a.comp_code = COALESCE($P{P0}, a.comp_code)
    AND a.emp_code = COALESCE($P{P1}, a.emp_code)
    AND a.process_cycle = COALESCE($P{P2}, a.process_cycle)
GROUP BY
    a.emp_code,
    a.emp_name,
    a.designation,
    a.basic_pay,
    b.process_description,
    b.pay_process_month,
    a.department,
    a.employee_bank,
    a.bank_branch,
    a.account_no,
    a.iban_number,
    a.date_of_join
ORDER BY a.emp_code;
]]>
	</queryString>
